CONFIG_MACH_XIAOMI_E10=y
CONFIG_INPUT_FINGERPRINT=y
CONFIG_FINGERPRINT_GOODIX_TA=y
CONFIG_FINGERPRINT_FPC_TEE=y
CONFIG_SND_SOC_TAS2559=y
CONFIG_SND_SOC_TAS2559_MAINLINE=n
CONFIG_TOUCHSCREEN_FTS_MI=y
# CONFIG_TOUCHSCREEN_FTS is not set
CONFIG_TOUCHSCREEN_NT36XXX_MI845=y
CONFIG_USE_BU64748=n
CONFIG_LEDS_LM3644=y

# Beryllium CPU
# OSM
CONFIG_CLOCK_CPU_OSM_SDM845=y
# CPUFREQ_HW
CONFIG_ARM_QCOM_CPUFREQ_HW=n
CONFIG_ARM_QCOM_DEVFREQ_FW=n
CONFIG_LEGACY_ENERGY_MODEL_DT=y

# Beryllium backlight
CONFIG_BACKLIGHT_QCOM_SPMI_WLED=y
CONFIG_BACKLIGHT_QCOM_WLED=n

# Haptics Beryllium
CONFIG_INPUT_QTI_HAPTICS=y
CONFIG_LEDS_QPNP_HAPTICS=n

# Kprobes
CONFIG_KPROBES=y
CONFIG_HAVE_KPROBES=y
CONFIG_KPROBE_EVENTS=y

# KernelSU_next
CONFIG_KSU=y

# Susfs
CONFIG_KSU_SUSFS=y
CONFIG_KSU_SUSFS_HAS_MAGIC_MOUNT=y

#
# RCU Debugging
#
CONFIG_DISABLE_TRACE_PRINTK=n

# DTBO
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# DebugFS
CONFIG_DEBUG_FS=n
CONFIG_MSM_TZ_LOG=n
CONFIG_CMA_DEBUGFS=n
CONFIG_REGMAP_ALLOW_WRITE_DEBUGFS=n
CONFIG_IPA_UT=n
CONFIG_IOMMU_TESTS=n
CONFIG_IOMMU_TESTS=n
CONFIG_QMP_DEBUGFS_CLIENT=n

# exFAT
CONFIG_EXFAT_FS=y

# Persistent store
CONFIG_PSTORE=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_RAM=y
CONFIG_CMDLINE="ramoops_memreserve=4M"
CONFIG_CMDLINE_EXTEND=y
CONFIG_BLK_WBT=y
CONFIG_BLK_WBT_SQ=y
CONFIG_MTD=y
CONFIG_MTD_BLOCK=y
CONFIG_MTD_OOPS=y
CONFIG_MTD_BLOCK2MTD=y
